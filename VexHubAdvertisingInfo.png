exe = syn or Fluxus
getcustomasset = getsynasset or getcustomasset
request = syn and syn.request or requet

local path = "vexman.png"
local url = ""
local result = request({
	Url = url,
	Method = 'GET'
})

if result.Success then
if isfile(path) then
if readfile(path) ~= result.Body then
writefile(path, result.Body)
end
else
writefile(path, result.Body)
end
else
print(result)
end

game:GetService("CoreGui").VexHubFolder.DragScreen.DragImage.ScriptAds.Image = getcustomasset("vexman.png")
